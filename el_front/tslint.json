{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended", "tslint:latest", "tslint-react"
	],
	"jsRules": {},
	"rulesDirectory": [],
	"rules": {
		"quotemark": [true, "single", "avoid-escape", "avoid-template"],
		"indent": [true, "tabs", 2],
		"interface-name": [true, "never-prefix"],
		"jsx-no-multiline-js": false,
		"no-submodule-imports": false
	}
}